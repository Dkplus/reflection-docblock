{
    "name": "dkplus/reflection-docblock",
    "description": "Reflection on PHP-DocBlocks",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oskar Pfeifer-Bley",
            "email": "oskar@programming-php.net"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Dkplus\\Reflection\\DocBlock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Dkplus\\Reflection\\DocBlock\\": "spec/",
            "test\\Dkplus\\Reflection\\DocBlock\\": "tests/"
        }
    },
    "require": {
        "hoa/compiler": "^3.0",
        "phpdocumentor/type-resolver": "^0.5.0",
        "phpdocumentor/reflection-common": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "phpspec/phpspec": "^4.0",
        "doctrine/annotations": "^1.4",
        "object-calisthenics/phpcs-calisthenics-rules": "^3.0.1",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "suggest": {
        "roave/better-reflection": "^2.0"
    }
}
